{
  "name": "skyhhjmk/rainyun-php-sdk",
  "description": "Used for easily managing Rainyun.com services on the PHP platform",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "RainYun\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "RainYun\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "skyhhjmk",
      "email": "admin@biliwind.com",
      "homepage": "https://www.biliwind.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.4",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.1",
    "psr/http-message": "^2.0"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^7.0 || ^7.5 || ^8.0",
    "guzzlehttp/psr7": "^2.6",
    "phpunit/phpunit": "^9.5 || ^10.0"
  },
  "scripts": {
    "test": "phpunit",
    "test-coverage": "phpunit --coverage-html coverage"
  }
}
